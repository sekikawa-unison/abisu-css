/*! ===================================
	ボタン
=================================== */
[class^="up-btn"]
	display inline-block
	margin-right 5px
	margin-bottom 5px
	a:hover
		opacity 1

.up-btn01
	a,
	span
		display inline-block
		padding 10px 20px
		background gray
		color #fff
		opacity 1
		cursor pointer
		transition 0.4s
	a:hover,
	span:hover
		opacity 0.5

.up-btn02
	a,
	span
		display inline-block
		padding 10px 20px
		background gray
		color #fff
		opacity 1
		cursor pointer
		transition 0.4s
		border 1px solid transparent
	a:hover,
	span:hover
		opacity 1
		color gray
		background #fff
		border-color gray

.up-btn03
	a,
	span
		display inline-block
		padding 10px 20px
		background gray
		color #fff
		opacity 1
		cursor pointer
		transition 0.4s
	a:hover,
	span:hover
		opacity 1
		background green


btn-cmn-setting01()
	display inline-block
	padding 1em
	cursor pointer
	background #e1e1e1
	color #666

.up-btn-grow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-grow a:hover,
.up-btn-grow a:focus,
.up-btn-grow a:active
	transform scale(1.1)

/*! Shrink */
.up-btn-shrink a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-shrink a:hover,
.up-btn-shrink a:focus,
.up-btn-shrink a:active
	transform scale(0.9)

/*! Pulse */
@keyframes up-btn-pulse
	25%
		transform scale(1.1)
	75%
		transform scale(0.9)

.up-btn-pulse a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-pulse a:hover,
.up-btn-pulse a:focus,
.up-btn-pulse a:active
	animation-name up-btn-pulse
	animation-duration 1s
	animation-timing-function linear
	animation-iteration-count infinite

/*! Pulse Grow */
@keyframes up-btn-pulse-grow
	to
		transform scale(1.1)

.up-btn-pulse-grow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-pulse-grow a:hover,
.up-btn-pulse-grow a:focus,
.up-btn-pulse-grow a:active
	animation-name up-btn-pulse-grow
	animation-duration 0.3s
	animation-timing-function linear
	animation-iteration-count infinite
	animation-direction alternate

/*! Pulse Shrink */
@keyframes up-btn-pulse-shrink
	to
		transform scale(0.9)

.up-btn-pulse-shrink a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-pulse-shrink a:hover,
.up-btn-pulse-shrink a:focus,
.up-btn-pulse-shrink a:active
	animation-name up-btn-pulse-shrink
	animation-duration 0.3s
	animation-timing-function linear
	animation-iteration-count infinite
	animation-direction alternate

/*! Push */
@keyframes up-btn-push
	50%
		transform scale(0.8)
	100%
		transform scale(1)

.up-btn-push a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-push a:hover,
.up-btn-push a:focus,
.up-btn-push a:active
	animation-name up-btn-push
	animation-duration 0.3s
	animation-timing-function linear
	animation-iteration-count 1

/*! Pop */
@keyframes up-btn-pop
	50%
		transform scale(1.2)

.up-btn-pop a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-pop a:hover,
.up-btn-pop a:focus,
.up-btn-pop a:active
	animation-name up-btn-pop
	animation-duration 0.3s
	animation-timing-function linear
	animation-iteration-count 1

/*! Rotate */
.up-btn-rotate a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-rotate a:hover,
.up-btn-rotate a:focus,
.up-btn-rotate a:active
	transform rotate(4deg)

/*! Float */
.up-btn-float a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-timing-function ease-out
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-float a:hover,
.up-btn-float a:focus,
.up-btn-float a:active
	transform translateY(-8px)

/*! Sink */
.up-btn-sink a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-timing-function ease-out
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-sink a:hover,
.up-btn-sink a:focus,
.up-btn-sink a:active
	transform translateY(8px)

/*! Hang */
@keyframes up-btn-hang
	0%
		transform translateY(8px)
	50%
		transform translateY(4px)
	100%
		transform translateY(8px)

@keyframes up-btn-hang-sink
	100%
		transform translateY(8px)

.up-btn-hang a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-hang a:hover,
.up-btn-hang a:focus,
.up-btn-hang a:active
	animation-name up-btn-hang-sink,up-btn-hang
	animation-duration 0.3s,1.5s
	animation-timing-function ease-out,ease-in-out
	animation-delay 0s,0.3s
	animation-iteration-count 1,infinite
	animation-direction normal,alternate
	animation-fill-mode forwards

/*! Skew */
.up-btn-skew a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-skew a:hover,
.up-btn-skew a:focus,
.up-btn-skew a:active
	transform skew(-10deg)

/*! Skew Forward */
.up-btn-skew-forward a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	transform-origin 0 100%
	btn-cmn-setting01()

.up-btn-skew-forward a:hover,
.up-btn-skew-forward a:focus,
.up-btn-skew-forward a:active
	transform skew(-10deg)

/*! Wobble Vertical */
@keyframes up-btn-wobble-vertical
	16.65%
		transform translateY(8px)
	33.3%
		transform translateY(-6px)
	49.95%
		transform translateY(4px)
	66.6%
		transform translateY(-2px)
	83.25%
		transform translateY(1px)
	100%
		transform translateY(0)

.up-btn-wobble-vertical a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-wobble-vertical a:hover,
.up-btn-wobble-vertical a:focus,
.up-btn-wobble-vertical a:active
	animation-name up-btn-wobble-vertical
	animation-duration 1s
	animation-timing-function ease-in-out
	animation-iteration-count 1

/*! Wobble Horizontal */
@keyframes up-btn-wobble-horizontal
	16.65%
		transform translateX(8px)
	33.3%
		transform translateX(-6px)
	49.95%
		transform translateX(4px)
	66.6%
		transform translateX(-2px)
	83.25%
		transform translateX(1px)
	100%
		transform translateX(0)

.up-btn-wobble-horizontal a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-wobble-horizontal a:hover,
.up-btn-wobble-horizontal a:focus,
.up-btn-wobble-horizontal a:active
	animation-name up-btn-wobble-horizontal
	animation-duration 1s
	animation-timing-function ease-in-out
	animation-iteration-count 1

/*! Wobble Top */
@keyframes up-btn-wobble-top
	16.65%
		transform skew(-12deg)
	33.3%
		transform skew(10deg)
	49.95%
		transform skew(-6deg)
	66.6%
		transform skew(4deg)
	83.25%
		transform skew(-2deg)
	100%
		transform skew(0)

.up-btn-wobble-top a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	transform-origin 0 100%
	btn-cmn-setting01()

.up-btn-wobble-top a:hover,
.up-btn-wobble-top a:focus,
.up-btn-wobble-top a:active
	animation-name up-btn-wobble-top
	animation-duration 1s
	animation-timing-function ease-in-out
	animation-iteration-count 1

/*! Wobble Bottom */
@keyframes up-btn-wobble-bottom
	16.65%
		transform skew(-12deg)
	33.3%
		transform skew(10deg)
	49.95%
		transform skew(-6deg)
	66.6%
		transform skew(4deg)
	83.25%
		transform skew(-2deg)
	100%
		transform skew(0)

.up-btn-wobble-bottom a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	transform-origin 100% 0
	btn-cmn-setting01()

.up-btn-wobble-bottom a:hover,
.up-btn-wobble-bottom a:focus,
.up-btn-wobble-bottom a:active
	animation-name up-btn-wobble-bottom
	animation-duration 1s
	animation-timing-function ease-in-out
	animation-iteration-count 1

/*! BORDER TRANSITIONS */
/*! Border Fade */
.up-btn-border-fade a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	box-shadow inset 0 0 0 4px #e1e1e1,0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow
	transform perspective(1px) translateZ(0)
	/*! Hack to improve aliasing on mobile/tablet devices */
	btn-cmn-setting01()

.up-btn-border-fade a:hover,
.up-btn-border-fade a:focus,
.up-btn-border-fade a:active
	box-shadow inset 0 0 0 4px #2098D1,0 0 1px rgba(0,0,0,0)
	/*! Hack to improve aliasing on mobile/tablet devices */

/*! Hollow */
.up-btn-hollow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	box-shadow inset 0 0 0 4px #e1e1e1,0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property background
	transform perspective(1px) translateZ(0)
	/*! Hack to improve aliasing on mobile/tablet devices */
	btn-cmn-setting01()

.up-btn-hollow a:hover,
.up-btn-hollow a:focus,
.up-btn-hollow a:active
	background none

/*! Trim */
.up-btn-trim a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-trim a:before
	position absolute
	top 4px
	right 4px
	bottom 4px
	left 4px
	border white solid 4px
	content ""
	opacity 0
	transition-duration 0.3s
	transition-property opacity

.up-btn-trim a:hover:before,
.up-btn-trim a:focus:before,
.up-btn-trim a:active:before
	opacity 1

/*! Outline Out */
.up-btn-outline-out a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-outline-out a:before
	position absolute
	top 0
	right 0
	bottom 0
	left 0
	border #e1e1e1 solid 4px
	content ""
	transition-duration 0.3s
	transition-property top,right,bottom,left

.up-btn-outline-out a:hover:before,
.up-btn-outline-out a:focus:before,
.up-btn-outline-out a:active:before
	top -8px
	right -8px
	bottom -8px
	left -8px

/*! Outline In */
.up-btn-outline-in a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-outline-in a:before
	position absolute
	top -16px
	right -16px
	bottom -16px
	left -16px
	border #e1e1e1 solid 4px
	content ""
	opacity 0
	transition-duration 0.3s
	transition-property top,right,bottom,left
	pointer-events none

.up-btn-outline-in a:hover:before,
.up-btn-outline-in a:focus:before,
.up-btn-outline-in a:active:before
	top -8px
	right -8px
	bottom -8px
	left -8px
	opacity 1

/*! Round Corners */
.up-btn-round-corners a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property border-radius
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-round-corners a:hover,
.up-btn-round-corners a:focus,
.up-btn-round-corners a:active
	border-radius 1em

/*! SHADOW/GLOW TRANSITIONS */
/*! Glow */
.up-btn-glow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-glow a:hover,
.up-btn-glow a:focus,
.up-btn-glow a:active
	box-shadow 0 0 8px rgba(0,0,0,0.6)

/*! Shadow */
.up-btn-shadow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-shadow a:hover,
.up-btn-shadow a:focus,
.up-btn-shadow a:active
	box-shadow 0 10px 10px -10px rgba(0,0,0,0.5)

/*! Grow Shadow */
.up-btn-grow-shadow a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow,transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-grow-shadow a:hover,
.up-btn-grow-shadow a:focus,
.up-btn-grow-shadow a:active
	box-shadow 0 10px 10px -10px rgba(0,0,0,0.5)
	transform scale(1.1)

/*! Box Shadow Outset */
.up-btn-box-shadow-outset a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-box-shadow-outset a:hover,
.up-btn-box-shadow-outset a:focus,
.up-btn-box-shadow-outset a:active
	box-shadow 2px 2px 2px rgba(0,0,0,0.6)

/*! Box Shadow Inset */
.up-btn-box-shadow-inset a
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	box-shadow inset 0 0 0 rgba(0,0,0,0.6),0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property box-shadow
	transform perspective(1px) translateZ(0)
	/*! Hack to improve aliasing on mobile/tablet devices */
	btn-cmn-setting01()

.up-btn-box-shadow-inset a:hover,
.up-btn-box-shadow-inset a:focus,
.up-btn-box-shadow-inset a:active
	box-shadow inset 2px 2px 2px rgba(0,0,0,0.6),0 0 1px rgba(0,0,0,0)
	/*! Hack to improve aliasing on mobile/tablet devices */

/*! Float Shadow */
.up-btn-float-shadow a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-float-shadow a: before
	position absolute
	top 100%
	left 5%
	z-index -1
	width 90%
	height 10px
	background -webkit-radial-gradient(center,ellipse,rgba(0,0,0,0.35) 0%,rgba(0,0,0,0) 80%)
	background radial-gradient(ellipse at center,rgba(0,0,0,0.35) 0%,rgba(0,0,0,0) 80%)
	content ""
	opacity 0
	/*! W3C */
	transition-duration 0.3s
	transition-property transform,opacity
	pointer-events none

.up-btn-float-shadow a:hover,
.up-btn-float-shadow a:focus,
.up-btn-float-shadow a:active
	transform translateY(-5px)
	/*! move the element up by 5px */

.up-btn-float-shadow a:hover:before,
.up-btn-float-shadow a:focus:before,
.up-btn-float-shadow a:active:before
	opacity 1
	transform translateY(5px)
	/*! move the element down by 5px (it will stay in place because it's attached to the element that also moves up 5px) */

/*! Shadow Radial */
.up-btn-shadow-radial a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-shadow-radial a:before,
.up-btn-shadow-radial a:after
	position absolute
	left 0
	box-sizing border-box
	width 100%
	height 5px
	background-repeat no-repeat
	content ""
	opacity 0
	transition-duration 0.3s
	transition-property opacity
	pointer-events none

.up-btn-shadow-radial a:before
	bottom 100%
	background -webkit-radial-gradient(50% 150%,ellipse,rgba(0,0,0,0.6) 0%,rgba(0,0,0,0) 80%)
	background radial-gradient(ellipse at 50% 150%,rgba(0,0,0,0.6) 0%,rgba(0,0,0,0) 80%)

.up-btn-shadow-radial a:after
	top 100%
	background -webkit-radial-gradient(50% -50%,ellipse,rgba(0,0,0,0.6) 0%,rgba(0,0,0,0) 80%)
	background radial-gradient(ellipse at 50% -50%,rgba(0,0,0,0.6) 0%,rgba(0,0,0,0) 80%)

.up-btn-shadow-radial a:hover:before,
.up-btn-shadow-radial a:focus:before,
.up-btn-shadow-radial a:active:before,
.up-btn-shadow-radial a:hover:after,
.up-btn-shadow-radial a:focus:after,
.up-btn-shadow-radial a:active:after
	opacity 1

/*! SPEECH BUBBLES */
/*! Bubble Top */
.up-btn-bubble-top a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-bubble-top a:before
	position absolute
	top 0
	left calc(50% - 10px)
	z-index -1
	border-width 0 10px 10px 10px
	border-style solid
	border-color transparent transparent #e1e1e1 transparent
	content ""
	transition-duration 0.3s
	transition-property transform
	pointer-events none

.up-btn-bubble-top a:hover:before,
.up-btn-bubble-top a:focus:before,
.up-btn-bubble-top a:active:before
	transform translateY(-10px)

/*! Bubble Right */
.up-btn-bubble-right a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-bubble-right a:before
	position absolute
	top calc(50% - 10px)
	right 0
	z-index -1
	border-width 10px 0 10px 10px
	border-style solid
	border-color transparent transparent transparent #e1e1e1
	content ""
	transition-duration 0.3s
	transition-property transform
	pointer-events none

.up-btn-bubble-right a:hover:before,
.up-btn-bubble-right a:focus:before,
.up-btn-bubble-right a:active:before
	transform translateX(10px)

/*! Bubble Bottom */
.up-btn-bubble-bottom a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-bubble-bottom a:before
	position absolute
	bottom 0
	left calc(50% - 10px)
	z-index -1
	border-width 10px 10px 0 10px
	border-style solid
	border-color #e1e1e1 transparent transparent transparent
	content ""
	transition-duration 0.3s
	transition-property transform
	pointer-events none

.up-btn-bubble-bottom a:hover:before,
.up-btn-bubble-bottom a:focus:before,
.up-btn-bubble-bottom a:active:before
	transform translateY(10px)

/*! Bubble Left */
.up-btn-bubble-left a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-bubble-left a:before
	position absolute
	top calc(50% - 10px)
	left 0
	z-index -1
	border-width 10px 10px 10px 0
	border-style solid
	border-color transparent #e1e1e1 transparent transparent
	content ""
	transition-duration 0.3s
	transition-property transform
	pointer-events none

.up-btn-bubble-left a:hover:before,
.up-btn-bubble-left a:focus:before,
.up-btn-bubble-left a:active:before
	transform translateX(-10px)

/*! Bubble Float Top */
.up-btn-bubble-float-top a
	position relative
	display inline-block
	box-shadow 0 0 1px rgba(0,0,0,0)
	vertical-align middle
	transition-duration 0.3s
	transition-property transform
	transform perspective(1px) translateZ(0)
	btn-cmn-setting01()

.up-btn-bubble-float-top a:before
	position absolute
	top 0
	left calc(50% - 10px)
	z-index -1
