/* フィード */
.fead-mv
  opacity 0
  &.mv
    opacity 1
    transition 0.5s ease-out
.fead-up
  opacity 0
  transform translateY(50px)
  &.mv
    opacity 1
    transform translateY(0)
    transition 0.5s ease-out
.fead-right
  opacity 0
  transform translateX(-20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s
.fead-left
  opacity 0
  transform translateX(20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s
.fead-text
  opacity 0
  &.mv
    opacity 1
    transition 0s
    transition-delay 0s
  .str
    opacity 0
    &.textin
      opacity 1
      transition 1s
.fead-cover
  position relative
  &:before
    content ""
    width 100%
    height 100%
    display block
    background #000
    position absolute
    top 0
    right 0
    z-index 15
  &.mv
    &:before
      width 0
      transition 0.8s
      transition-delay 0.6s
.fead-order
  & > *
    opacity 0
    transform translateY(50px)
  & > *.mv
    opacity 1
    transform translateY(0)
    transition 0.5s ease-out

