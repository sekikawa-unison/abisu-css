@charset "utf-8"
/* version 1.1 */

[class*="heightLine"]
  display block

/* フィード */
.fead-mv
  opacity 0
  &.mv
    opacity 1
    transition 0.5s ease-out
.fead-up
  opacity 0
  transform translateY(50px)
  &.mv
    opacity 1
    transform translateY(0)
    transition 0.5s ease-out
.fead-right
  opacity 0
  transform translateX(-20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s
.fead-left
  opacity 0
  transform translateX(20%)
  &.mv
    opacity 1
    transform translateX(0)
    transition 1s
.fead-text
  opacity 0
  &.mv
    opacity 1
    transition 0s
    transition-delay 0s
  .str
    opacity 0
    &.textin
      opacity 1
      transition 1s
.fead-cover
  position relative
  &:before
    content ""
    width 100%
    height 100%
    display block
    background #000
    position absolute
    top 0
    right 0
    z-index 15
  &.mv
    &:before
      width 0
      transition 0.8s
      transition-delay 0.6s
.fead-order
  & > *
    opacity 0
    transform translateY(50px)
  &.mv
    opacity 1
    transform translateY(0)
    transition 0.5s ease-out


/* font-weight */
.up-fw-light
  font-weight lighter
.up-fw-normal
  font-weight normal
.up-fw-bold
  font-weight bold

/* 画像比率固定 */
[class*="up-ofi"]
  position relative
  display block
  width 100%
  img
    width 100%
    height 100% !important
    /* IE対策 */
    font-family "object-fit: cover; object-position: 50% 50%;"
    object-fit cover
    object-position 50% 50%

/* 子要素のaタグをブロック化 */
.up-adb a
  display block

/* ３点リーダー（１行） */
.up-reader1
  overflow hidden
  text-overflow ellipsis
  white-space nowrap

/* 正円 */
.up-circle
  overflow hidden
  border-radius 50%


